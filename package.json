{
  "name": "maptext",
  "version": "0.1.0",
  "description": "Associate text to image regions for text search and within-image copy-paste",
  "main": "maptext-index.js",
  "scripts": {
    "prepare": "npm run copy",
    "open": "open-cli http://localhost:8050/ && npm run start",
    "start": "json-server maptext.json --middlewares server/access.js server/text-search.js --snapshots snapshots --id name --watch --static . --port 8050",
    "eslint": "eslint --ext=js,md,html .",
    "lint": "npm run eslint",
    "copy-im": "cp -a ../imagemaps/dist/. node_modules/imagemaps/dist/",
    "copy:im": "cp node_modules/imagemaps/dist/index.esm.js external/imagemaps/dist/index.esm.js",
    "copy:tippy.js": "rollup -c",
    "copy:core-js-bundle": "cp node_modules/core-js-bundle/minified.js external/core-js-bundle/minified.js",
    "copy:jquery": "cp node_modules/jquery/dist/jquery.js external/jquery/dist/jquery.js",
    "copy:regenerator-runtime": "echo 'Copying regenerator-runtime for simple-prefs' && cp node_modules/regenerator-runtime/runtime.js external/regenerator-runtime/runtime.js",
    "copy": "run-p copy:im copy:tippy.js copy:core-js-bundle copy:jquery copy:regenerator-runtime",
    "test:unit": "mocha --require chai/register-expect --require @babel/register test/unit/mapTextSearch.js",
    "testcafe-ui": "testcafe chrome test/ui/ui.js",
    "testcafe-accessibility": "testcafe chrome test/ui/accessibility.js",
    "testcafe-only": "run-s testcafe-ui testcafe-accessibility",
    "testcafe": "run-p -r start testcafe-only",
    "test:ui": "echo \"Running separate tests due to testcafe issues in running both...\" && npm run testcafe",
    "test": "run-s eslint test:unit test:ui"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/brettz9/maptext.git"
  },
  "keywords": [
    "map",
    "area",
    "image",
    "text"
  ],
  "author": "Brett Zamir",
  "contributors": [],
  "browserslist": [
    "cover 100%"
  ],
  "license": "MIT",
  "engines": {
    "node": ">=12.0.0"
  },
  "bugs": {
    "url": "https://github.com/brettz9/maptext/issues"
  },
  "homepage": "https://github.com/brettz9/maptext#readme",
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/eslint-parser": "^7.14.7",
    "@babel/plugin-transform-modules-commonjs": "^7.14.5",
    "@babel/register": "^7.14.5",
    "@brettz9/eslint-plugin": "^1.0.3",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "axe-core": "^4.2.3",
    "axe-testcafe": "^3.0.0",
    "babel-plugin-dynamic-import-node-babel-7": "^2.0.7",
    "chai": "^4.3.4",
    "core-js-bundle": "^3.15.2",
    "eslint": "^7.30.0",
    "eslint-config-ash-nazg": "^30.1.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-array-func": "^3.1.7",
    "eslint-plugin-compat": "^3.9.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-html": "^6.1.2",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jsdoc": "^35.4.1",
    "eslint-plugin-markdown": "^2.2.0",
    "eslint-plugin-no-unsanitized": "^3.1.5",
    "eslint-plugin-no-use-extend-native": "^0.5.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-sonarjs": "^0.9.1",
    "eslint-plugin-standard": "^4.1.0",
    "eslint-plugin-testcafe": "^0.2.1",
    "eslint-plugin-unicorn": "^34.0.1",
    "imagemaps": "git+https://github.com/brettz9/ImageMaps.git#remove-all-shapes",
    "jquery": "^3.6.0",
    "json-server": "^0.16.3",
    "kld-intersections": "https://github.com/thelonious/kld-intersections#development",
    "mocha": "^9.0.2",
    "npm-run-all": "^4.1.5",
    "open-cli": "^7.0.0",
    "regenerator-runtime": "^0.13.7",
    "rollup": "^2.52.8",
    "testcafe": "^1.15.0",
    "tippy.js": "^6.3.1"
  },
  "dependencies": {
    "@passport-next/passport": "git+https://github.com/brettz9/passport.git#promises",
    "@passport-next/passport-local": "git+https://github.com/brettz9/passport-local.git#es6-linting",
    "form-serialization": "^0.11.0",
    "htteepee": "^0.5.0",
    "jamilih": "^0.54.0",
    "load-stylesheets": "^0.10.0",
    "multipronged": "^0.1.0",
    "simple-prefs": "^0.7.0"
  }
}
