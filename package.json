{
  "name": "maptext",
  "version": "0.1.0",
  "description": "Associate text to image regions for text search and within-image copy-paste",
  "main": "maptext-index.js",
  "scripts": {
    "open": "open-cli http://localhost:8050/ && npm run start",
    "start": "json-server maptext.json --middlewares server/text-search.js --snapshots snapshots --id name --watch --static . --port 8050",
    "eslint": "eslint .",
    "copy-im": "cp -a ../imagemaps/dist/. node_modules/imagemaps/dist/",
    "tippy-rollup": "rollup -c",
    "copy": "npm run tippy-rollup && cp node_modules/core-js-bundle/minified.js external/core-js-bundle/minified.js && cp node_modules/jquery/dist/jquery.js external/jquery/dist/jquery.js && cp node_modules/regenerator-runtime/runtime.js external/regenerator-runtime/runtime.js",
    "test:unit": "mocha --require @babel/register test/unit/mapTextSearch.js",
    "test": "npm run eslint && testcafe chrome test/ui/*.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/brettz9/maptext.git"
  },
  "keywords": [
    "map",
    "area",
    "image",
    "text"
  ],
  "author": "Brett Zamir",
  "contributors": [],
  "browserslist": [
    "cover 100%"
  ],
  "license": "MIT",
  "engines": {},
  "bugs": {
    "url": "https://github.com/brettz9/maptext/issues"
  },
  "homepage": "https://github.com/brettz9/maptext#readme",
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/plugin-transform-modules-commonjs": "^7.4.4",
    "@babel/register": "^7.4.4",
    "@mysticatea/eslint-plugin": "^10.0.3",
    "axe-testcafe": "^1.1.0",
    "babel-eslint": "^10.0.1",
    "babel-plugin-dynamic-import-node-babel-7": "^2.0.7",
    "chai": "^4.2.0",
    "core-js-bundle": "^3.1.3",
    "eslint": "^5.16.0",
    "eslint-config-ash-nazg": "^6.1.1",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-compat": "^3.1.1",
    "eslint-plugin-eslint-comments": "^3.1.1",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-jsdoc": "^7.2.3",
    "eslint-plugin-markdown": "^1.0.0",
    "eslint-plugin-no-use-extend-native": "^0.4.0",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-promise": "^4.1.1",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-testcafe": "^0.2.1",
    "eslint-plugin-unicorn": "^9.0.0",
    "json-server": "^0.15.0",
    "kld-intersections": "^0.6.0",
    "mocha": "^6.1.4",
    "open-cli": "^5.0.0",
    "regenerator-runtime": "^0.13.2",
    "rollup": "^1.13.1",
    "rollup-plugin-node-resolve": "^5.0.1",
    "testcafe": "^1.2.0",
    "typescript": "^3.5.1"
  },
  "dependencies": {
    "connect": "^3.7.0",
    "form-serialization": "^0.10.0",
    "htteepee": "^0.5.0",
    "imagemaps": "^1.0.1",
    "jamilih": "^0.45.0",
    "jquery": "^3.4.1",
    "load-stylesheets": "^0.8.0",
    "simple-prefs": "^0.1.0",
    "tippy.js": "^4.3.2"
  }
}
